<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Vital Signs Water Laboratory Analysis Survey – 13 February 2015 v1</h:title>
    <model>
      <instance>
        <VS_Water_Quality_13.02.2015 id="water_lab_13_feb_2015_v1" version="1">
          <start/>
          <end/>
          <today/>
          <metadata>
            <meta_display>
              <country/>
              <sample_note/>
              <analyst_first_name/>
              <analyst_last_name/>
              <data_entry_date/>
            </meta_display>
          </metadata>
          <calibration/>
          <cl1_grp>
            <cl1_1>yes</cl1_1>
            <cl1_2>yes</cl1_2>
            <cl2_0/>
            <cl2_1>yes</cl2_1>
            <cl2_no_1>yes</cl2_no_1>
            <cl2_no_2>yes</cl2_no_2>
            <cl2_temp/>
          </cl1_grp>
          <water_smp_page/>
          <water_smp_rpt jr:template="">
            <water_smp_grp>
              <water_smp_note/>
              <water_smp_code/>
              <water_smp-code_label/>
              <water_smp_ph/>
              <water_smp_cdty/>
              <water_smp_no3/>
              <water_smp_po4/>
              <ws1_1/>
              <ws1_mpn/>
            </water_smp_grp>
          </water_smp_rpt>
          <meta>
            <instanceID/>
          </meta>
        </VS_Water_Quality_13.02.2015>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/VS_Water_Quality_13.02.2015/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/VS_Water_Quality_13.02.2015/end" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/VS_Water_Quality_13.02.2015/today" type="date"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/metadata/meta_display/country" required="true()" type="select1"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/metadata/meta_display/sample_note" type="string"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/metadata/meta_display/analyst_first_name" required="true()" type="string"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/metadata/meta_display/analyst_last_name" required="true()" type="string"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/metadata/meta_display/data_entry_date" type="date"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/calibration" readonly="true()" type="string"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/cl1_grp/cl1_1" type="select1"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/cl1_grp/cl1_2" type="select1"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_0" type="select1"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_1" type="select1"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_no_1" type="select1"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_no_2" type="select1"/>
      <bind constraint=". &gt;= 0 and . &lt;= 100" nodeset="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_temp" required="true()" type="decimal"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/water_smp_page" readonly="true()" type="string"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_note" readonly="true()" type="string"/>
      <bind constraint="regex(., '^[0-9]{4}$')" jr:constraintMsg="Please enter the 4 numeral code that would be added to e.g TZA_WS_xxxx" nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_code" required="true()" type="string"/>
      <bind calculate="concat( /VS_Water_Quality_13.02.2015/metadata/meta_display/country , '-', 'WS', '-',  /VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_code )" nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp-code_label" type="string"/>
      <bind constraint="(. &gt;= 0 and . &lt;= 14) and regex(., '^-?\d*\.\d$')" jr:constraintMsg="Please enter pH value between ( 0 – 14) and to 1 decimal places (Required)!" nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_ph" required="true()" type="decimal"/>
      <bind constraint="(. &gt;= 0 and . &lt;= 20) and regex(., '^-?\d*\.\d\d$')" jr:constraintMsg="Please enter Conductivity value between ( 0 – 20) and to 2 decimal places (Required)!" nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_cdty" type="decimal"/>
      <bind constraint="(. &gt;= 0 and . &lt;= 2000) and regex(., '^-?\d*\.\d$')" jr:constraintMsg="Please enter NO3 value between ( 0 – 2000) and to 1 decimal places (Required)!" nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_no3" type="decimal"/>
      <bind constraint=". &gt;= 0 and . &lt;= 200" jr:constraintMsg="Please enter PO4 value between ( 0 – 200)!" nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_po4" type="decimal"/>
      <bind nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/ws1_1" type="select"/>
      <bind constraint=". &gt;= 0 and . &lt;= 100" nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/ws1_mpn" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/VS_Water_Quality_13.02.2015/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/VS_Water_Quality_13.02.2015/metadata">
      <group appearance="field-list" ref="/VS_Water_Quality_13.02.2015/metadata/meta_display">
        <select1 ref="/VS_Water_Quality_13.02.2015/metadata/meta_display/country">
          <label>Country</label>
          <item>
            <label>Ghana</label>
            <value>GHA</value>
          </item>
          <item>
            <label>Tanzania</label>
            <value>TZA</value>
          </item>
          <item>
            <label>Uganda</label>
            <value>UGA</value>
          </item>
        </select1>
        <input ref="/VS_Water_Quality_13.02.2015/metadata/meta_display/sample_note">
          <label>Sample Notes</label>
          <hint>Optional</hint>
        </input>
        <input ref="/VS_Water_Quality_13.02.2015/metadata/meta_display/analyst_first_name">
          <label>Analysts First Name</label>
        </input>
        <input ref="/VS_Water_Quality_13.02.2015/metadata/meta_display/analyst_last_name">
          <label>Analysts Last Name</label>
        </input>
        <input ref="/VS_Water_Quality_13.02.2015/metadata/meta_display/data_entry_date">
          <label>Date of data entry</label>
        </input>
      </group>
    </group>
    <input ref="/VS_Water_Quality_13.02.2015/calibration">
      <label>CALIBRATION OF INSTRUMENT</label>
      <hint>Except for recording water temperature, one should tick off each step of calibration process.</hint>
    </input>
    <group appearance="field-list" ref="/VS_Water_Quality_13.02.2015/cl1_grp">
      <select1 ref="/VS_Water_Quality_13.02.2015/cl1_grp/cl1_1">
        <label>EC Calibration</label>
        <item>
          <label>Yes</label>
          <value>1</value>
        </item>
        <item>
          <label>No</label>
          <value>0</value>
        </item>
      </select1>
      <select1 ref="/VS_Water_Quality_13.02.2015/cl1_grp/cl1_2">
        <label>PO4 Blank</label>
        <item>
          <label>Yes</label>
          <value>1</value>
        </item>
        <item>
          <label>No</label>
          <value>0</value>
        </item>
      </select1>
      <select1 ref="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_0">
        <label>pH 4 Calibration</label>
        <item>
          <label>Yes</label>
          <value>1</value>
        </item>
        <item>
          <label>No</label>
          <value>0</value>
        </item>
      </select1>
      <select1 ref="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_1">
        <label>pH 7 Calibration</label>
        <item>
          <label>Yes</label>
          <value>1</value>
        </item>
        <item>
          <label>No</label>
          <value>0</value>
        </item>
      </select1>
      <select1 ref="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_no_1">
        <label>NO3 High Calibration</label>
        <item>
          <label>Yes</label>
          <value>1</value>
        </item>
        <item>
          <label>No</label>
          <value>0</value>
        </item>
      </select1>
      <select1 ref="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_no_2">
        <label>NO3 Low Calibration</label>
        <item>
          <label>Yes</label>
          <value>1</value>
        </item>
        <item>
          <label>No</label>
          <value>0</value>
        </item>
      </select1>
      <input ref="/VS_Water_Quality_13.02.2015/cl1_grp/cl2_temp">
        <label>Recorded Water Temperature(Degrees Celsius)</label>
        <hint>Temperature value between ( 0 – 100)</hint>
      </input>
    </group>
    <input ref="/VS_Water_Quality_13.02.2015/water_smp_page">
      <label>WATER SAMPLE DATA details ahead.</label>
    </input>
    <group ref="/VS_Water_Quality_13.02.2015/water_smp_rpt">
      <label>WATER SAMPLE DATA</label>
      <repeat nodeset="/VS_Water_Quality_13.02.2015/water_smp_rpt">
        <group appearance="field-list" ref="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp">
          <input ref="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_note">
            <label>WATER SAMPLE DATA</label>
          </input>
          <input appearance="numbers" ref="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_code">
            <label>Sample Code</label>
            <hint>Enter the 4 numeral code e.g 0004</hint>
          </input>
          <input ref="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_ph">
            <label>pH</label>
            <hint>pH value between ( 0 – 14) and to 1 decimal places (Required) e.g 0.1</hint>
          </input>
          <input ref="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_cdty">
            <label>Conductivity (mS/m)</label>
            <hint>Conductivity value between ( 0 – 20) and to 2 decimal places (Required) E.g 0.22</hint>
          </input>
          <input ref="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_no3">
            <label>NO3 (mg/l)</label>
            <hint>NO3 value between ( 0 – 2000) and to 1 decimal places (Required) e.g 2.1</hint>
          </input>
          <input ref="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/water_smp_po4">
            <label>PO4 (mg/l)</label>
            <hint>PO4 value between ( 0 – 200) E.g 10</hint>
          </input>
          <select ref="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/ws1_1">
            <label>Ecoli</label>
            <hint>Tick if colour has changed in each compartment</hint>
            <item>
              <label>10ml</label>
              <value>10ml</value>
            </item>
            <item>
              <label>30ml</label>
              <value>30ml</value>
            </item>
            <item>
              <label>56ml</label>
              <value>50ml</value>
            </item>
            <item>
              <label>3ml</label>
              <value>3ml</value>
            </item>
            <item>
              <label>1ml</label>
              <value>1ml</value>
            </item>
          </select>
          <input ref="/VS_Water_Quality_13.02.2015/water_smp_rpt/water_smp_grp/ws1_mpn">
            <label>MPN Per 100 ml</label>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
