      <bind nodeset="/VS_Eplot_full_v20/metadata/meta_display/landscape_real" type="string"/>
      <bind constraint="regex(.,'^[0-9]{3}$')" nodeset="/VS_Eplot_full_v20/metadata/meta_display/plot" required="true()" type="string"/>
      <bind calculate="concat( /VS_Eplot_full_v20/metadata/meta_display/country , '-',  /VS_Eplot_full_v20/metadata/meta_display/landscape , '-',  /VS_Eplot_full_v20/metadata/meta_display/plot )" nodeset="/VS_Eplot_full_v20/metadata/meta_display/eplot-code" type="string"/>
      <bind nodeset="/VS_Eplot_full_v20/metadata/meta_display/radius" type="decimal"/>
