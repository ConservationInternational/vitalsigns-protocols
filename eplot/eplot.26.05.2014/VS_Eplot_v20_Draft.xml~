      <bind calculate="36" nodeset="/VS_Eplot_full_v20/subplot_group_count" readonly="true()" type="string"/>
      <bind calculate="position(..)" nodeset="/VS_Eplot_full_v20/subplot_group/plotnum" type="string"/>
      <bind nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group" relevant="position(..) = 1" required="true()"/>
      <bind nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_note" readonly="true()" type="string"/>
      <bind nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_start" required="true()" type="time"/>
      <bind constraint="decimal-time( /VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_end )-decimal-time( /VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_start ) &gt; 0" jr:constraintMsg="Please, the GPS times can't be equal" nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_end" required="true()" type="time"/>
      <bind nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_ns" required="true()" type="select1"/>
      <bind constraint="regex(., '^-?\d*\.\d\d\d\d\d$') and . &lt; 90" jr:constraintMsg="Please enter 5 decimal places!" nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_lat" required="true()" type="string"/>
      <bind nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_ew" required="true()" type="select1"/>
      <bind constraint="regex(., '^-?\d*\.\d\d\d\d\d$') and . &lt; 90" jr:constraintMsg="Please enter 5 decimal places!" nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_long" required="true()" type="string"/>
      <bind nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_accuracy" required="true()" type="decimal"/>
      <bind nodeset="/VS_Eplot_full_v20/subplot_group/gpsse_group/gpsse_measurement_counts" required="true()" type="int"/>
      <bind nodeset="/VS_Eplot_full_v20/subplot_group/landscape_se" relevant="position(..) = 1"/>
      <bind constraint=". &gt;= 0 and . &lt;= 96" jr:constraintMsg="must be between 0 - 96" nodeset="/VS_Eplot_full_v20/subplot_group/landscape_se/densiometer_se_n" required="true()" type="int"/>
      <bind constraint=". &gt;= 0 and . &lt;= 96" jr:constraintMsg="must be between 0 - 96" nodeset="/VS_Eplot_full_v20/subplot_group/landscape_se/densiometer_se_e" required="true()" type="int"/>
      <bind constraint=". &gt;= 0 and . &lt;= 96" jr:constraintMsg="must be between 0 - 96" nodeset="/VS_Eplot_full_v20/subplot_group/landscape_se/densiometer_se_s" required="true()" type="int"/>
      <bind constraint=". &gt;= 0 and . &lt;= 96" jr:constraintMsg="must be between 0 - 96" nodeset="/VS_Eplot_full_v20/subplot_group/landscape_se/densiometer_se_w" required="true()" type="int"/>
      <bind calculate="( /VS_Eplot_full_v20/subplot_group/landscape_se/densiometer_se_n  +  /VS_Eplot_full_v20/subplot_group/landscape_se/densiometer_se_e  +  /VS_Eplot_full_v20/subplot_group/landscape_se/densiometer_se_w  +  /VS_Eplot_full_v20/subplot_group/landscape_se/densiometer_se_s ) div 96" nodeset="/VS_Eplot_full_v20/subplot_group/landscape_se/den_se_total" type="string"/>
